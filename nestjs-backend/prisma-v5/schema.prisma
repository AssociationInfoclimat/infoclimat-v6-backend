generator client {
  provider = "prisma-client-js"
  output = "../prisma-v5/v5-database-client-types"
}

datasource v5 {
  provider = "mysql"
  url      = env("V5_DATABASE_URL")
}

model accueil_extremes_alertes {
  id               BigInt                           @id @default(autoincrement()) @v5.UnsignedBigInt
  id_station       String                           @v5.VarChar(30)
  date             DateTime                         @v5.Date
  deleted          accueil_extremes_alertes_deleted
  nbr_signalements Int
  fixed            accueil_extremes_alertes_fixed

  @@unique([id_station, date])
}

model api_previ_acces {
  id        BigInt   @id @default(autoincrement()) @v5.UnsignedBigInt
  dh        DateTime @v5.DateTime(0)
  ip        Int      @v5.UnsignedInt
  useragent String?  @v5.VarChar(255)
  referer   String?  @v5.VarChar(255)
  key       String?  @v5.VarChar(255)
  type      Boolean

  @@index([dh, ip])
}

model comptes_tokens {
  id              Int      @id @default(autoincrement()) @v5.UnsignedInt
  id_compte       Int      @v5.UnsignedInt
  token           String   @v5.VarChar(255)
  dh_creation     DateTime @v5.DateTime(0)
  dh_expiration   DateTime @v5.DateTime(0)
  ip_creation     String   @v5.VarChar(50)
  uagent_creation String   @v5.VarChar(255)

  @@index([id_compte])
  @@index([token])
}

model comptes {
  id                    Int                          @id @default(autoincrement())
  dh_creation           DateTime                     @v5.DateTime(0)
  pseudo                String                       @v5.VarChar(30)
  mdp                   String                       @v5.TinyText
  statuts               String                       @v5.VarChar(255)
  dh_derniere_connexion DateTime                     @v5.DateTime(0)
  ip_derniere_connexion String                       @v5.VarChar(15)
  id_forum              Int
  pseudo_forum          String                       @v5.VarChar(30)
  confidentialite       comptes_confidentialite
  MA_statutanc          String                       @v5.TinyText
  MA_statut             Int                          @v5.TinyInt
  MA_warn               Int                          @v5.MediumInt
  MA_pays               String                       @v5.TinyText
  MA_cp                 String                       @v5.TinyText
  MA_notes              String                       @v5.MediumText
  MA_obstot             Int                          @v5.MediumInt
  MA_obsdel             Int                          @v5.MediumInt
  MA_r_tmp              comptes_MA_r_tmp             @default(dbgenerated("1"))
  MA_r_vent             comptes_MA_r_vent            @default(dbgenerated("1"))
  MA_r_pluie            comptes_MA_r_pluie           @default(dbgenerated("1"))
  MA_r_banni            String                       @v5.TinyText
  MA_a_banni            String                       @v5.TinyText
  MA_dh_inscription     DateTime                     @v5.DateTime(0)
  MA_participation_vdt  comptes_MA_participation_vdt
  MA_obstoty            Int                          @v5.MediumInt
  MA_obsdely            Int                          @v5.MediumInt
  parametres            String                       @v5.Text
  dh_liaison_forum      DateTime?                    @v5.DateTime(0)
  mdp_hash              String                       @v5.VarChar(255)
  user_twitter          String                       @default("") @v5.VarChar(255)
  user_facebook         String                       @default("") @v5.VarChar(255)
  user_instagram        String                       @default("") @v5.VarChar(255)

  @@unique([pseudo])
  @@index([MA_participation_vdt])
}

enum comptes_MA_r_tmp {
  ZERO @map("0")
  ONE  @map("1")
}

enum comptes_MA_r_vent {
  ZERO @map("0")
  ONE  @map("1")
}

enum comptes_MA_r_pluie {
  ZERO @map("0")
  ONE  @map("1")
}

enum comptes_MA_participation_vdt {
  ZERO @map("0")
  ONE  @map("1")
}

enum comptes_confidentialite {
  ZERO  @map("0")
  ONE   @map("1")
  TWO   @map("2")
  THREE @map("3")
  FOUR  @map("4")
}

enum accueil_extremes_alertes_deleted {
  ZERO @map("0")
  ONE  @map("1")
}

enum personnes_coordonnees_confidentialite {
  ZERO  @map("0")
  ONE   @map("1")
  TWO   @map("2")
  THREE @map("3")
  FOUR  @map("4")
}

enum accueil_extremes_alertes_fixed {
  ZERO @map("0")
  ONE  @map("1")
}

